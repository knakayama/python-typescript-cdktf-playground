{
  "name": "infrastructure",
  "packageManager": "yarn@3.2.2",
  "version": "0.0.1",
  "description": "Infrastructure",
  "scripts": {
    "lint:ec": "ec --exclude 'node_modules|\\.git|cdktf\\.out|\\.yarn'",
    "lint:eslint": "eslint --ext .ts src",
    "lint:prettier": "prettier --check 'src/**/*.{ts,yaml,md,json}' '*.{md,yaml,json,js,ts}'",
    "lint": "yarn lint:ec && yarn lint:prettier && yarn lint:eslint",
    "postinstall": "husky install",
    "build": "zx ./scripts/build.mjs",
    "gitleaks": "gitleaks detect -v",
    "test": "jest",
    "cdktf": "cdktf",
    "cdktf:diff": "cdktf diff mng_account",
    "cdktf:deploy": "(cd cdktf.out/stacks/mng_account && terraform apply plan)",
    "cdktf:dev": "aws-vault exec dev -- cdktf",
    "cdktf:dev:diff": "aws-vault exec dev -- cdktf diff dev_account",
    "cdktf:dev:deploy": "(cd cdktf.out/stacks/dev_account && aws-vault exec dev -- terraform apply plan)"
  },
  "engines": {
    "node": ">=18.7.0",
    "npm": "please_use_yarn_instead"
  },
  "private": true,
  "repository": "https://github.com/knakayama/python-lambda-playground.git",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.103",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cdktf-cli": "^0.14.1",
    "editorconfig-checker": "^4.0.2",
    "esbuild": "^0.15.7",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "type-fest": "^2.18.0",
    "typescript": "^4.8.2",
    "zx": "^7.0.8"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.165.0",
    "@cdktf/provider-aws": "^11.0.0",
    "axios": "^0.27.2",
    "cdktf": "^0.14.1",
    "constructs": "^10.1.65",
    "param-case": "^3.0.4",
    "snake-case": "^3.0.4"
  }
}
