{
  "name": "python-typescript-cdktf-playground",
  "packageManager": "yarn@3.3.1",
  "version": "0.0.1",
  "description": "Python Typescript CDKTF Playground",
  "scripts": {
    "lint:ec": "ec",
    "lint:eslint": "eslint --ext .ts,.mts,.mjs,.js,.cjs .",
    "lint:prettier": "prettier --check '**/*.{ts,yaml,md,json,mts,js,cjs,mjs,toml}'",
    "lint": "yarn lint:ec && yarn lint:prettier && yarn lint:eslint",
    "gitleaks": "gitleaks detect -v",
    "postinstall": "husky install",
    "infra": "cdktf",
    "infra:get": "yarn cdktf get -o ./src/lib",
    "infra:synth": "yarn cdktf synth",
    "infra:init": "(cd cdktf.out/stacks/$@ && terraform init)",
    "infra:plan": "(cd cdktf.out/stacks/$@ && terraform plan -out=plan)",
    "infra:synth:plan": "yarn infra:synth && yarn infra:plan",
    "infra:apply": "(cd cdktf.out/stacks/$@ && terraform apply plan)",
    "infra:plan:apply": "(cd cdktf.out/stacks/$@ && terraform plan -out=plan && terraform apply plan)",
    "infra:synth:plan:apply": "yarn infra:synth && yarn infra:plan:apply",
    "app:lint": "./scripts/lintApp.mts",
    "app:build": "./scripts/buildApp.mts && ./scripts/buildLayer.mts"
  },
  "engines": {
    "node": ">=19.3.0",
    "npm": "please_use_yarn_instead"
  },
  "private": true,
  "repository": "https://github.com/knakayama/python-lambda-playground.git",
  "type": "module",
  "devDependencies": {
    "@swc/core": "^1.3.24",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "cdktf-cli": "^v0.15.0-pre.38",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.3",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "micromatch": "^4.0.5",
    "prettier": "^2.7.1",
    "prettier-plugin-toml": "^0.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "type-fest": "^3.5.0",
    "typescript": "^4.8.2",
    "zx": "^7.1.1"
  },
  "dependencies": {
    "@cdktf/provider-aws": "^11.0.0",
    "cdktf": "^v0.15.0-pre.38",
    "constructs": "^10.1.65",
    "param-case": "^3.0.4",
    "snake-case": "^3.0.4"
  }
}
