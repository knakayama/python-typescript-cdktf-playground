{
  "name": "python-aws-playground",
  "packageManager": "yarn@3.3.1",
  "version": "0.0.1",
  "description": "Python AWS Playground",
  "scripts": {
    "lint:app": "ts-node-esm --swc ./scripts/lintApp.mts",
    "lint:ec": "ec",
    "lint:eslint": "eslint --ext .ts --ext .mts src scripts",
    "lint:prettier": "prettier --check '{src,app,scripts}/**/*.{ts,yaml,md,json,mts}' '*.{md,yaml,json,js,ts}'",
    "lint": "yarn lint:ec && yarn lint:prettier && yarn lint:eslint",
    "gitleaks": "gitleaks detect -v",
    "test": "jest",
    "cdktf": "cdktf",
    "cdktf:get": "cdktf get -o src/lib",
    "cdktf:diff": "cdktf diff api",
    "cdktf:deploy": "(cd cdktf.out/stacks/api && terraform apply plan)",
    "build:app": "(cd app && poetry install)",
    "build:layer": "ts-node-esm --swc ./scripts/buildLayer.mts",
    "postinstall": "husky install"
  },
  "engines": {
    "node": ">=18.7.0",
    "npm": "please_use_yarn_instead"
  },
  "private": true,
  "repository": "https://github.com/knakayama/python-lambda-playground.git",
  "type": "module",
  "devDependencies": {
    "@swc/core": "^1.3.24",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "cdktf-cli": "^0.14.1",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.2",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "type-fest": "^3.5.0",
    "typescript": "^4.8.2",
    "zx": "^7.1.1"
  },
  "dependencies": {
    "@cdktf/provider-aws": "^11.0.0",
    "cdktf": "^0.14.1",
    "constructs": "^10.1.65",
    "param-case": "^3.0.4",
    "snake-case": "^3.0.4"
  }
}
