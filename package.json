{
  "name": "infrastructure",
  "packageManager": "yarn@3.3.1",
  "version": "0.0.1",
  "description": "Infrastructure",
  "scripts": {
    "lint:ruff": "(cd app && poetry run ruff .)",
    "lint:black": "(cd app && poetry run black app --verbose)",
    "lint:mypy": "(cd app && poetry run mypy app)",
    "lint:app": "yarn lint:ruff && yarn lint:black && yarn lint:mypy",
    "lint:ec": "ec",
    "lint:eslint": "eslint --ext .ts src",
    "lint:prettier": "prettier --check '{src,app}/**/*.{ts,yaml,md,json.toml}' '*.{md,yaml,json,js,ts,toml}'",
    "lint": "yarn lint:ec && yarn lint:prettier && yarn lint:eslint",
    "gitleaks": "gitleaks detect -v",
    "test": "jest",
    "cdktf": "cdktf",
    "cdktf:diff": "cdktf diff api",
    "cdktf:deploy": "(cd cdktf.out/stacks/api && terraform apply plan)",
    "build:app": "(cd app && poetry install)",
    "build:layer": "zx ./scripts/build_layer.mjs",
    "postinstall": "husky install"
  },
  "engines": {
    "node": ">=18.7.0",
    "npm": "please_use_yarn_instead"
  },
  "private": true,
  "repository": "https://github.com/knakayama/python-lambda-playground.git",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cdktf-cli": "^0.14.1",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.2",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "type-fest": "^3.5.0",
    "typescript": "^4.8.2",
    "zx": "^7.1.1"
  },
  "dependencies": {
    "@cdktf/provider-aws": "^11.0.0",
    "cdktf": "^0.14.1",
    "constructs": "^10.1.65",
    "param-case": "^3.0.4",
    "snake-case": "^3.0.4"
  }
}
